generator users_Client {
  provider = "prisma-client-js"
}

datasource db_Users {
  provider = "postgresql"
  url      = env("PRISMA_DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  name      String
  password  String   @default("password123")
  
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}